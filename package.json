{
  "name": "universal-react-application-sample",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/konojunya/universal-react-application-sample.git",
  "author": "konojunya <junya03dance@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p build:watch serve",
    "serve": "wait-on build/server.js && pm2 start pm2-local.json --no-daemon",
    "build": "npm-run-all -p build:client build:server",
    "build:watch": "npm-run-all -p build:client:watch build:server:watch",
    "build:client": "webpack-cli --config webpack.config.js",
    "build:client:watch": "yarn build:client --watch",
    "build:server": "tsc --project .",
    "build:server:watch": "yarn build:server --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.7.0",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.0",
    "@types/react-router-config": "^1.1.3",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-thunk": "^2.1.0",
    "@types/serialize-javascript": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^3.5.0",
    "ts-loader": "^6.0.0",
    "typescript": "3.1.6",
    "wait-on": "^3.2.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
